{
  "expo": {
    "name": "startWars",
    "slug": "startWars",
    "version": "1.0.0",
    "scheme": "your-app-scheme",
    "orientation": "portrait",
    "icon": "./shared/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./shared/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./shared/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.startWars.startWars"
    },
    "web": {
      "favicon": "./shared/assets/favicon.png"
    },
    "plugins": ["expo-router"],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "617bb114-ab6a-48f2-89c6-d5714b62085e"
      }
    }
  }
}
